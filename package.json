{
  "private": true,
  "name": "edge-hn",
  "version": "0.0.0",
  "main": "dist/worker.js",
  "scripts": {
    "clean": "rm -rf dist",
    "postinstall": "npm run build",
    "build": "esbuild src/index.ts --bundle --target=es2020 --outfile=dist/worker.js --define:global=self --define:DEBUG=false",
    "watch": "esbuild src/index.ts --bundle --target=es2020 --outfile=dist/worker.js --define:global=self --define:DEBUG=true --sourcemap --watch",
    "build-deno": "esbuild src/deno-index.ts --bundle --format=esm --target=es2020 --outfile=dist/deno.js --define:global=self --tsconfig=tsconfig.deno.json --inject:env.js --define:DEBUG=false ",
    "watch-deno": "esbuild src/deno-index.ts --bundle --format=esm --target=es2020 --outfile=dist/deno.js --define:global=self --tsconfig=tsconfig.deno.json --inject:env.js --define:DEBUG=true --sourcemap --watch",
    "build-sw": "esbuild src/sw-index.ts --bundle --format=esm --target=es2020 --outfile=sw.js --define:global=self --tsconfig=tsconfig.sw.json --inject:env.js --define:DEBUG=false ",
    "watch-sw": "esbuild src/sw-index.ts --bundle --format=esm --target=es2020 --outfile=sw.js --define:global=self --tsconfig=tsconfig.sw.json --inject:env.js --define:DEBUG=true --sourcemap --watch",
    "serve": "wrangler dev",
    "serve-deno": "deno run --allow-read --allow-write --allow-net --location=http://localhost:8000 dist/deno.js",
    "serve-sw": "serve -l 5000",
    "dev": "npm run watch & npm run serve",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "author": "Florian Klampfer <mail@qwtel.com>",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^2.1.0",
    "@types/node": "^14.14.34",
    "esbuild": "^0.9.2",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@worker-tools/cloudflare-kv-storage": "^0.7.3",
    "@worker-tools/deno-fetch-event-adapter": "github:worker-tools/deno-fetch-event-adapter",
    "@worker-tools/deno-kv-storage": "^0.3.6",
    "@worker-tools/encrypted-cookie-store": "^0.1.5",
    "@worker-tools/html": "github:worker-tools/html",
    "@worker-tools/json-fetch": "^2.0.6",
    "@worker-tools/location-polyfill": "github:worker-tools/location-polyfill",
    "@worker-tools/request-cookie-store": "^0.4.2",
    "@worker-tools/response-creators": "^1.0.8",
    "@worker-tools/signed-cookie-store": "^0.1.5",
    "@worker-tools/stripe-webhook": "^1.1.3",
    "abortcontroller-polyfill": "^1.7.1",
    "base64-encoding": "^0.14.3",
    "date-fns": "^2.19.0",
    "kv-storage-polyfill": "^2.0.0",
    "msgpackr": "^1.2.7",
    "negotiated": "^1.0.2",
    "tiny-request-router": "^1.2.2",
    "typed-array-utils": "^0.2.2",
    "typeson": "^5.18.2",
    "typeson-registry": "^1.0.0-alpha.39",
    "uuid-class": "^0.12.3",
    "whatwg-stream-to-async-iter": "github:qwtel/whatwg-stream-to-async-iter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worker-tools/edge-hn.git"
  },
  "bugs": {
    "url": "https://github.com/worker-tools/edge-hn/issues"
  },
  "homepage": "https://github.com/worker-tools/edge-hn#readme",
  "description": ""
}

{
  "private": true,
  "name": "edge-hn",
  "version": "0.1.0-pre.0",
  "main": "dist/worker.js",
  "scripts": {
    "clean": "rm -rf dist",
    "postinstall": "npm run build",
    "build": "npm run build-sw && npm run build-cf && npm run build-deno",
    "build-cf": "esbuild src/entry/cf.ts --bundle --target=es2022 --outfile=dist/worker.js --define:global=self --tsconfig=tsconfig.json --define:DEBUG=false --define:SW=false",
    "watch-cf": "esbuild src/entry/cf.ts --bundle --target=es2022 --outfile=dist/worker.js --define:global=self --tsconfig=tsconfig.json --define:DEBUG=true --define:SW=false --sourcemap --watch",
    "build-deno": "esbuild src/entry/deno.ts --bundle --format=esm --target=es2022 --outfile=dist/deno.js --define:global=self --tsconfig=tsconfig.deno.json --define:DEBUG=false --define:SW=false --define:self.WORKER_LOCATION=\\\"https://worker-news.deno.dev\\\"",
    "watch-deno": "esbuild src/entry/deno.ts --bundle --format=esm --target=es2022 --outfile=dist/deno.js --define:global=self --tsconfig=tsconfig.deno.json --define:DEBUG=true --define:SW=false --define:self.WORKER_LOCATION=\\\"http://localhost:8000\\\" --sourcemap --watch",
    "build-sw": "esbuild src/entry/sw.ts --bundle --target=es2020 --outfile=public/sw.js --define:global=self --tsconfig=tsconfig.sw.json --define:DEBUG=false --define:SW=true ",
    "watch-sw": "esbuild src/entry/sw.ts --bundle --target=es2020 --outfile=public/sw.js --define:global=self --tsconfig=tsconfig.sw.json --define:DEBUG=true --define:SW=true --sourcemap --watch",
    "serve": "miniflare --watch",
    "serve-wrangler": "wrangler dev",
    "serve-deno": "deno run --allow-read --allow-write --allow-net --watch --location=http://localhost:8000 dist/deno.js ",
    "dev": "npm run watch & npm run serve",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "author": "Florian Klampfer <mail@qwtel.com>",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.9.0",
    "@types/node": "^17.0.31",
    "esbuild": "^0.14.38",
    "miniflare": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@download/blockies": "github:qwtel/blockies",
    "@miniflare/html-rewriter": "^2.4.0",
    "@qwtel/p-queue-browser": "^6.6.2",
    "@types/html-escaper": "^3.0.0",
    "@worker-tools/cloudflare-kv-storage": "^0.8.0",
    "@worker-tools/deno-fetch-event-adapter": "^1.0.5",
    "@worker-tools/deno-kv-storage": "^0.4.4",
    "@worker-tools/encrypted-cookie-store": "^0.2.1",
    "@worker-tools/extendable-promise": "0.2.0-pre.8",
    "@worker-tools/html": "^2.0.0-pre.8",
    "@worker-tools/html-rewriter": "^0.1.0-pre.12",
    "@worker-tools/json-fetch": "2.1.0-pre.5",
    "@worker-tools/json-stream": "0.1.0-pre.3",
    "@worker-tools/location-polyfill": "^0.4.0",
    "@worker-tools/middleware": "^0.1.0-pre.16",
    "@worker-tools/parsed-html-rewriter": "^0.1.11",
    "@worker-tools/request-cookie-store": "^0.5.0-pre.9",
    "@worker-tools/resolvable-promise": "^0.2.0-pre.4",
    "@worker-tools/response-creators": "^1.0.8",
    "@worker-tools/router": "0.2.0-pre.6",
    "@worker-tools/signed-cookie-store": "^0.3.0-pre.7",
    "@worker-tools/stream-response": "^0.1.0-pre.0",
    "@worker-tools/stripe-webhook": "^1.1.4",
    "base64-encoding": "^0.14.3",
    "cookie-store-interface": "^0.1.1",
    "date-fns": "^2.28.0",
    "event-target-to-async-iter": "^0.2.1",
    "html-escaper": "^3.0.3",
    "html-rewriter-wasm": "^0.4.1",
    "kv-storage-interface": "^0.2.0",
    "kv-storage-polyfill": "^2.0.0",
    "linkedom": "^0.14.8",
    "msgpackr": "^1.5.7",
    "negotiated": "^1.0.2",
    "parse-domain": "^7.0.0",
    "ts-compose": "^1.0.0",
    "ts-functional-pipe": "^3.1.2",
    "ts-pipe-compose": "^0.2.1",
    "type-fest": "^2.12.2",
    "typed-array-utils": "^0.2.2",
    "typeson": "^6.1.0",
    "typeson-registry": "^1.0.0-alpha.39",
    "urlpattern-polyfill": "^4.0.3",
    "uuid-class": "^0.12.3",
    "whatwg-stream-to-async-iter": "0.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worker-tools/edge-hn.git"
  },
  "bugs": {
    "url": "https://github.com/worker-tools/edge-hn/issues"
  },
  "homepage": "https://github.com/worker-tools/edge-hn#readme",
  "description": ""
}
